    "version": "0.2.0",
    "configurations": [
        {
            "type": "pwa-chrome",
            "request": "launch",
            "name": "Launch Chrome against localhost",
            "url": "http://localhost/E-JavaScript/Calculadora",
            "webRoot": "${workspaceFolder}"
