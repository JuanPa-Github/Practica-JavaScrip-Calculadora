        {
            "type": "chrome",
            "port": 80,
            "request": "attach",
            "name": "Attach to Chrome",
            "url": "http://localhost/E-JavaScript/Calculadora/index.html",
            "webRoot": "${workspaceFolder}"
        }
    ]
}